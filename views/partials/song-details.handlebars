<a
  href='/songs/{{id}}'
  class='p-8 rounded-lg bg-indigo-800 hover:bg-indigo-500 mr-11 mt-3 mb-0'
  id='song-{{id}} songsetAtt'
  data-tempo='{{tempo}}'
  data-danceability='{{danceability}}'
  data-valence='{{valence}}'
  data-speechiness='{{speechiness}}'
  data-sentiment='{{sentiment}}'
>
  <div class='flex flex-row w-full justify-between pr-10'>
    <p class='text-lg font-semibold text-sky-400'>{{title}}</p>
    <p class='text-base font-light text-sy-500'>{{artist}}</p>
  </div>
</a>